%TEMPLATE_MODE HTML5
# ------------------------------------------------------------
%CONTEXT
bool = true
# ------------------------------------------------------------
%INPUT
<p th:alt="${'Hello'}">...</p>
<p th:alt="${'Hello'} + ${''}">...</p>
<p th:alt="${'Hello'} + ${2}">...</p>
<p th:alt="|${'Hello'} ${2}|">...</p>
<p th:alt="|${'Hello'} ${bool}|">...</p>
<p th:alt="|${'Hello'} false|">...</p>
<p th:alt="false">...</p>
<p th:alt="2314">...</p>
# ------------------------------------------------------------
%OUTPUT
<p alt="Hello">...</p>
<p alt="Hello">...</p>
<p alt="Hello[2]">...</p>
<p alt="Hello [2]">...</p>
<p alt="Hello [true]">...</p>
<p alt="Hello false">...</p>
<p alt="[false]">...</p>
<p alt="[2314]">...</p>

